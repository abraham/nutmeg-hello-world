    "serve": "npx nutmeg serve .",
    "test": "npx nutmeg test .",
    "watch": "npx nutmeg watch ."
  },
  "dependencies": {
    "@nutmeg/seed": "0.4.1"
  },
  "devDependencies": {
    "@nutmeg/cli": "0.9.2",
    "@types/chai": "4.1.0",
    "@types/mocha": "2.2.46",
    "@webcomponents/webcomponentsjs": "1.0.22"
  }
