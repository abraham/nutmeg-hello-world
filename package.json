{
  "name": "@nutmeg/hello-world",
  "version": "0.9.3",
  "description": "Hello World Nutmeg Web Component",
  "main": "dist/hello-world.js",
  "module": "dist/hello-world.js",
  "unpkg": "dist/hello-world.min.js",
  "types": "dist/hello-world.d.ts",
  "directories": {
    "test": "test"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "keywords": [
    "hello-world",
    "nutmeg"
  ],
  "license": "MIT",
  "scripts": {
    "build": "npx nutmeg build .",
    "build:production": "npx nutmeg build . --production",
